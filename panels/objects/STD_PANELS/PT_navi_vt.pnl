"DragEnter" "main(string information, int dragType)
{
  string dpe = dpSubStr(information, DPSUB_DP);
  
  dropAccept((dpExists(information) && symb_isDptTrendable(dpTypeName(dpe)))); 
}"
"DragDrop" "main(string information, int dragType)
{
  string dpe = dpSubStr(information, DPSUB_DP), dpt;
  int action;
  
  //DebugN(dpe, dragType);
  
  if (!dpExists(dpe))
    return;
  
  dpt = dpTypeName(dpe);
  
  switch (dragType)
  {
    case DRAG_COPY:
      symb_EvaluatePopup(makeDynString(), 2000, dpe, dpt);  //Hinzufügen
      break;
    case DRAG_MOVE: 
      symb_EvaluatePopup(makeDynString(), 2999, dpe, dpt);  //Löschen
      symb_EvaluatePopup(makeDynString(), 2000, dpe, dpt);  //Hinzufügen
      break;
    default: break;
  }
}"
