 E E "main()
{
  time mainPanelOpened, lastLayerOperation;
  
  // if the last display time in the _Ui_. datapoint is the same than in the ApplicationProps
  // the same mainpanel is still in use as it was the last time, the layer control was opened
  // if not, a new panel was opened with default operations what leads to visibility of all
  // existing layers - if a strict layer control is needed for all mainpanels, this can be 
  // implemented by applying the visible-states for all layers same time with the opening
 
  dpGet(myUiDpName()+\".RootPanelOrigOn.PanelName:_online.._stime\",mainPanelOpened,
  		\"ApplicationProperties.panels.mainpanel.layer_lastpanel_time:_online.._value\",lastLayerOperation);
  
  
  
  if(mainPanelOpened != lastLayerOperation)
  {
    dpSet(\"ApplicationProperties.panels.mainpanel.layer_lastpanel_time:_original.._value\",mainPanelOpened,
    	  \"ApplicationProperties.panels.mainpanel.layer_1_visible:_original.._value\",TRUE,
  		  \"ApplicationProperties.panels.mainpanel.layer_2_visible:_original.._value\",TRUE,
  		  \"ApplicationProperties.panels.mainpanel.layer_3_visible:_original.._value\",TRUE,
  		  \"ApplicationProperties.panels.mainpanel.layer_4_visible:_original.._value\",TRUE,
  		  \"ApplicationProperties.panels.mainpanel.layer_5_visible:_original.._value\",TRUE,
  		  \"ApplicationProperties.panels.mainpanel.layer_6_visible:_original.._value\",TRUE,
  		  \"ApplicationProperties.panels.mainpanel.layer_7_visible:_original.._value\",TRUE,
  		  \"ApplicationProperties.panels.mainpanel.layer_8_visible:_original.._value\",TRUE);   
  }  

  dpConnect(\"showActiveLayers\", \"ApplicationProperties.panels.mainpanel.layer_1_visible:_online.._value\",
  								\"ApplicationProperties.panels.mainpanel.layer_2_visible:_online.._value\",
  								\"ApplicationProperties.panels.mainpanel.layer_3_visible:_online.._value\",
  								\"ApplicationProperties.panels.mainpanel.layer_4_visible:_online.._value\",
  								\"ApplicationProperties.panels.mainpanel.layer_5_visible:_online.._value\",
  								\"ApplicationProperties.panels.mainpanel.layer_6_visible:_online.._value\",
  								\"ApplicationProperties.panels.mainpanel.layer_7_visible:_online.._value\",
  								\"ApplicationProperties.panels.mainpanel.layer_8_visible:_online.._value\",
  								\"ApplicationProperties.panels.mainpanel.layer_names:_online.._value\");
}

showActiveLayers(string dp1, bool L1,
                 string dp2, bool L2,
                 string dp3, bool L3,
                 string dp4, bool L4,
                 string dp5, bool L5,
                 string dp6, bool L6,
                 string dp7, bool L7,
                 string dp8, bool L8,
                 string dp9, dyn_string names)
{
  setMultiValue(\"vis1\",\"visible\",L1,
  				\"vis2\",\"visible\",L2,
  				\"vis3\",\"visible\",L3,
  				\"vis4\",\"visible\",L4,
  				\"vis5\",\"visible\",L5,
  				\"vis6\",\"visible\",L6,
  				\"vis7\",\"visible\",L7,
  				\"vis8\",\"visible\",L8,
  				\"LayerButton1\",\"text\",\"Layer &1  \"+names[1],
  				\"LayerButton2\",\"text\",\"Layer &2  \"+names[2],
