changeLayerVisibility(int iLayer)
{
  bool state;
  getValue(\"vis\"+iLayer, \"visible\", state);
  
  if(state)
    da_LayerOffPanelInModule(iLayer, currentPanelName.text, \"mainModule_\"+iModuleNumber);
  else
    da_LayerOnPanelInModule(iLayer, currentPanelName.text, \"mainModule_\"+iModuleNumber);
  
  dpSet(\"ApplicationProperties.panels.mainpanel.layer_\"+iLayer+\"_visible:_original.._value\", !state);
}" 0
