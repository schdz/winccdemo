main()
{
  dyn_float sValues;
  dynClear(sValues);
  bOk=1;
  dpSet(\"PC_Production_Control.LED_Scanner.green:_original.._value\",1,
        \"PC_Production_Control.LED_Scanner.red:_original.._value\",0,
        \"PC_Production_Control.LED_Scanner.value_scanner:_original.._value\",sValues);
  dpConnect(\"workLEDScanner\",0,\"PC_Production_Control.LED_Scanner.red:_online.._value\");
}
workLEDScanner(string dp1,bool boRed)
{
  float value;
  dyn_float sValues;
  int i , iLenMax=10;
 
  dynClear(sValues); 
  if(boRed)
  {
    for(i=1;i<=iLenMax;i++)
    {
      value=-1.7+3.4*(rand())/32767.0;
      if(value>-1.0 && value<1.0 && i==5)
        value=1+(rand())/(2*32767.0);
      dynAppend(sValues,value);
    } 
    delay(0,500);
    bOk=0;
    dpSet(\"PC_Production_Control.LED_Scanner.green:_original.._value\",1,
          \"PC_Production_Control.LED_Scanner.red:_original.._value\",0,
          \"PC_Production_Control.LED_Scanner.value_scanner:_original.._value\",sValues);
  }
  else if(bOk)
  {
    for(i=1;i<=iLenMax;i++)
    {
      value=-1.7+3.4*(rand())/32767.0;
      if(value<-1.0 || value>1.0)
        value=-1+(rand())/(32767.0);
      dynAppend(sValues,value);
    } 
    dpSet(\"PC_Production_Control.LED_Scanner.value_scanner:_original.._value\",sValues);
  }
  else
    bOk=1;
  
    
