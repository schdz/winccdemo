bool boProduction=1;
bool boWaitNewParts = false;
bool boWaitRobot = false;
bool boLEDSensor = false;
main()
{
  int j;
  float x,y;
  
  for(j=1;j<10;j++)
          setValue(\"object_\"+j,\"visible\",1); 
  setValue(\"object_0\",\"visible\",0);
  for(j=1;j<6;j++)
  {
    moveParts(0,1,j);
    getValue(\"Ellipse\"+j+\"_1\",  \"position\",x,y);
    setValue(\"Ellipse\"+j+\"_2\",  \"position\",x,y);
  }
  
  dpGet(\"PC_Production_Control.total_production:_online.._value\",iTotalProduction,
        \"PC_Production_Control.total_loss:_online.._value\",iTotalLoss);
  
  if(iTotalProduction>=999999) 
  {
    iTotalProduction=0;
    iTotalLoss=0;
    dpSet(\"PC_Production_Control.total_production:_original.._value\",iTotalProduction,
          \"PC_Production_Control.total_loss:_original.._value\",iTotalLoss);
 
  }

  dpSet(\"PC_Production_Control.Robot_Control.start_Simulation:_original.._value\",1,
        \"PC_Production_Control.Robot_Control.move_parts:_original.._value\",0);

  dpConnect(\"workProduction\",\"PC_Production_Control.production_on:_online.._value\");
  dpConnect(\"moveRoboter\",\"PC_Production_Control.Robot_Control.start_Simulation:_online.._value\");
  dpConnect(\"workNewParts\",0,\"PC_Production_Control.Robot_Control.move_parts:_online.._value\");
  dpConnect(\"simValues\",\"PC_Production_Control.Robot_Control.start_Simulation:_online.._value\");  
  dpConnect(\"waitRobot\", \"ScaraRobot.move:_online.._value\",  
                           \"PC_Production_Control.Robot_Control.LED_Sensor:_online.._value\");
  startProduction();
}

waitRobot(string dp1, bool b1, string dp2, bool b2)
{
  boWaitRobot = b1;
  boLEDSensor = b2;
}

workProduction(string dp1,bool boOn)
{
  boProduction=boOn;
}  

startProduction()
{
  int value;
  int iLauf1=1,iLauf2=-10,iLauf3=-20,iLauf4=-30,iLauf5=-40,iLauf6=-50;
  int i,iObject;
  int iProdPath,iProdPath1=1,iProdPath2=1,iProdPath3=1,iProdPath4=1,iProdPath5=1,iProdPath6=1;
  bool bOk=1;
  
  
  while(1)
  {
    if(!boProduction)
    {
      delay(0,500);
    }
    else
    {
      iLauf1++;
      iLauf2++;
      iLauf3++;
      iLauf4++;
      iLauf5++;
      iLauf6++;
    
      for(iObject=1;iObject<=6;iObject++)
      {
        if(iObject==1) 
        { 
          i=iLauf1;
          iProdPath=iProdPath1;       
        } 
        else if(iObject==2) 
        { 
          i=iLauf2;
          iProdPath=iProdPath2;       
        }
        else if(iObject==3) 
        { 
          i=iLauf3;
          iProdPath=iProdPath3;       
        } 
        else if(iObject==4) 
        { 
          i=iLauf4;
          iProdPath=iProdPath4;       
        }
        else if(iObject==5) 
        { 
          i=iLauf5;
          iProdPath=iProdPath5;       
        } 
        else if(iObject==6) 
        { 
          i=iLauf6;
          iProdPath=iProdPath6;       
        }  
      
        value=i*10;
        if(value<0) value=0;
        else if(value==270)
        { 
          //the scanner detect a bad part. It activate the shuter
          if(iProdPath == 1)
          { 
            if (iObject == 4)
              dpSet(\"PC_Production_Control.LED_Scanner.green:_original.._value\",0,
                    \"PC_Production_Control.LED_Scanner.red:_original.._value\",1);
            else if (iObject == 5)
              dpSet(\"PC_Production_Control.LED_Scanner.green:_original.._value\",1,
                    \"PC_Production_Control.LED_Scanner.red:_original.._value\",0);
          }
        }
        else if(value==280 && iProdPath == 2) //now will the scaraRobot open the claw
        {
          dpSet(\"PC_Production_Control.Robot_Control.LED_Sensor:_original.._value\",1);
          //setValue(\"Ellipse\"+iObject+\"_2\",\"visible\",1);
        }
        else if(value>=140 && value<360 && iProdPath == 3)
          setValue(\"Ellipse\"+iObject+\"_1\",  \"visible\",0);
        
        else if(value>=340 && value<360 && iProdPath == 2)
          setMultiValue(\"Ellipse\"+iObject+\"_1\",  \"visible\",0,
                        \"Ellipse\"+iObject+\"_2\",  \"visible\",0);
        else if(value>=360)
        {   
          if(iProdPath==1 && iObject != 4)
            iProdPath=2;
          else if(iProdPath==1 && iObject == 4) //this part is bad, activate the pusher
          {
            iProdPath=3;            
            iTotalLoss++;
            dpSet(\"PC_Production_Control.pusher.on:_original.._value\",1,
                  \"PC_Production_Control.pusher.LED_red:_original.._value\",1,
                  \"PC_Production_Control.pusher.LED_green:_original.._value\",0,
                  \"PC_Production_Control.total_loss:_original.._value\",iTotalLoss);
          }
          else
          {
            if(iProdPath==2)
            {
              iTotalProduction++;
              if(iTotalProduction>=999999) 
              {
                iTotalProduction=0;
                iTotalLoss=0;
                dpSet(\"PC_Production_Control.total_production:_original.._value\",iTotalProduction,
                      \"PC_Production_Control.total_loss:_original.._value\",iTotalLoss);
               }
               else
                 dpSet(\"PC_Production_Control.total_production:_original.._value\",iTotalProduction);
            }
            iProdPath=1;
          }
          if(iObject==1) 
          {  
             iProdPath1=iProdPath;
             iLauf1=1;
          }
          else if(iObject==2) 
          {  
             iProdPath2=iProdPath;       
             iLauf2=1;
          } 
           else if(iObject==3) 
          {  
             iProdPath3=iProdPath;       
             iLauf3=1;
          }
          else if(iObject==4) 
          {  
             iProdPath4=iProdPath;       
             iLauf4=1;
          } 
          else if(iObject==5) 
          {  
             iProdPath5=iProdPath;       
             iLauf5=1;
          }
          else if(iObject==6) 
          {  
             iProdPath6=iProdPath;       
             iLauf6=1;
          }
        }
        moveParts(value,iProdPath,iObject);
        if(iObject!=4 && value==20 && iProdPath==1)
          setMultiValue(\"Ellipse\"+iObject+\"_1\",  \"visible\",1,
                        \"Ellipse\"+iObject+\"_2\",  \"visible\",1);
         else if(iObject==4 && value==20 && iProdPath==1) 
            setValue(\"Ellipse\"+iObject+\"_1\",  \"visible\",1);

        delay(0,050);
      }//end of for(...)
    }
  }//end of while(1)
}

moveParts(int value,int iProdPath,int iObject)
{
  float  percentage = (value >= 360) ? 0.0 : (value / 360.0 * 100.0),
         x, y, angle;

  getMultiValue(\"prodPath\"+iProdPath,\"positionOnPath\", percentage, x, y, angle);

  if(iProdPath==2 && value >= 280 && value < 360)
    setMultiValue(\"Ellipse\"+iObject+\"_1\",  \"position\", x, y,
   	              \"Ellipse\"+iObject+\"_1\",  \"rotation\", angle,
   			      \"Ellipse\"+iObject+\"_2\", \"position\", x, y,
   			      \"Ellipse\"+iObject+\"_2\",  \"rotation\", angle);
  else
    setMultiValue(\"Ellipse\"+iObject+\"_1\",  \"position\", x, y,
   	              \"Ellipse\"+iObject+\"_1\",  \"rotation\", angle);
}

moveRoboter(string dp1, bool boStart)
{
  bool boMove,boWait;
  int iObject=1;
  float x,y,refPosX, refPosY;
  
  getValue(\"scaraLink0\",\"position\",refPosX, refPosY); 
  
  while(boStart)
  {
    if(!boProduction)
    {
      delay(0,500);
    }
    else
    {
      dyn_anytype daResponse;
   
      boWaitRobot = true;
      dpSetWait(\"ScaraRobot.clawopen:_original.._value\",1);
      dpSet(\"ScaraRobot.move:_original.._value\",1);
     
      while (boWaitRobot)
        delay(0, 100);

      getMultiValue(\"object_\"+iObject,\"position\",x,y);
      x=x-refPosX;
      y=-refPosY+y;
    
      setValue(\"object_0\",\"visible\",0);

      boWaitRobot = true;
      dpSet(\"ScaraRobot.xpos:_original.._value\",x,
            \"ScaraRobot.ypos:_original.._value\",y,
            \"ScaraRobot.move:_original.._value\",1);

      while (boWaitRobot)
        delay(0, 100);

      dpSetWait(\"ScaraRobot.clawopen:_original.._value\",0);

      while (boWaitNewParts)
        delay(0, 100);

      boWaitRobot = true;
      dpSet(\"ScaraRobot.move:_original.._value\",1);

      while (boWaitRobot)
        delay(0, 100);

      boWaitRobot = true;
      dpSet(\"ScaraRobot.xpos:_original.._value\",-10,
            \"ScaraRobot.ypos:_original.._value\",180,
            \"ScaraRobot.move:_original.._value\",1);

      setMultiValue(\"object_0\",\"visible\",1,
                    \"object_\"+iObject,\"visible\",0);
    
      if(iObject == 9)
      {
        dpSet(\"PC_Production_Control.Robot_Control.move_parts:_original.._value\",1);
        boWaitNewParts = true;
      }
      
      while (boWaitRobot || !boLEDSensor)
        delay(0, 100);
     
      setValue(\"object_0\",\"visible\",0);

      boWaitRobot = true;
      dpSet(\"ScaraRobot.clawopen:_original.._value\",1,
            \"PC_Production_Control.Robot_Control.LED_Sensor:_original.._value\",0,
            \"ScaraRobot.move:_original.._value\",1);
      
      while (boWaitRobot)
        delay(0, 100);

      iObject++;    
      if(iObject==10)
        iObject=1;
    }
  }
}

workNewParts(string dp1,bool boMove)
{
  int i,j;
  int value,iLauf1=22,iLauf2=4;
  int iObject;
  bool bOk;

  boWaitNewParts = boMove;
 
  if(!boMove)
    return;
 
  moveParts2(220,4,1);
  moveParts2(40,4,2);
  
  while(1)
  {
  
    for(iObject=1;iObject<=2;iObject++)
    {
      if(iObject==1) 
      { 
        i=iLauf1;
      } 
      else if(iObject==2) 
      { 
        i=iLauf2;       
      }
      
      
      value=i*10;
      if(value>360)
      {
      
        if(iObject==1) 
        { 
          i=iLauf1=1;
        } 
        else if(iObject==2) 
        { 
          i=iLauf2=1;       
        }
        
      }
      if(iObject==1 && value==40)
      {
        return; 
      }
      else if(iObject==1 && value==360)
      {
        setValue(\"PartsGroup_1\",\"visible\",0);
        for(j=1;j<10;j++)
          setValue(\"object_\"+j,\"visible\",1); 
        dpSet(\"PC_Production_Control.Robot_Control.move_parts:_original.._value\",0);
      }
      moveParts2( value,4,iObject);
      if(iObject==1 && value>360)
        setValue(\"PartsGroup_1\",\"visible\",1);
      delay(0,100);
    }
    iLauf1++;
    iLauf2++;

  }
  
}
moveParts2(int value,int iProdPath,int iObject)
{
  float  percentage = (value >= 360) ? 0.0 : (value / 360.0 * 100.0),
         x, y, angle;
  float  xRobot,yRobot;
 
  getValue(\"prodPath\"+iProdPath,\"positionOnPath\", percentage, x, y, angle);
                   
  setValue(\"PartsGroup_\"+iObject,  \"position\", x, y);
}

