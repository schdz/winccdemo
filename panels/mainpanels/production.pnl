"float alpha;		// angle between arm1 (r1) and arm2 (r2)
float arm1Length, arm2Length, refPosX, refPosY;

main()
{
  int ldummy;

  setValue(\"posInd\",\"visible\",0);
  
  // Reference position for the robot ist the center link element
  // Length of both arms is read from helper shapes (lines)
  getMultiValue(\"armLength1\",\"size\",arm1Length,ldummy,
  				      \"armLength2\",\"size\",arm2Length,ldummy,
    			      \"scaraLink0\",\"position\",refPosX, refPosY); 
    
   dpSet(\"ScaraRobot.xpos:_original.._value\",-90, 
        \"ScaraRobot.ypos:_original.._value\",90,
        \"ScaraRobot.clawopen:_original.._value\",1,
        \"ScaraRobot.move:_original.._value\",1,
        \"PC_Production_Control.Robot_Control.LED_Sensor:_original.._value\",0);
  dpConnect(\"workRobot\",\"ScaraRobot.move:_online.._value\");
}

workRobot(string dp1,bool boMoveRobot)
{
  // ToDo. alle potentiellen div/0 rausschmeissen !!! Q3 und Q4 unterstützen
  // optimal kurzen weg wählen, 
  if(!boMoveRobot)
    return;
  else
  {
    float angle1, 
    	  angle2, 
  		  angle1a,	// complimentary angle of the 2nd solution
  		  angle2a,    // complimentary angle of the 2nd solution
          angleOpt1,  // optimate angle betwen angle1 and angle1a for the robot-trajectory
          angleOpt2,  // optimate angle betwen angle2 and angle2a for the robot-trajectory
  		  oldangle1,
  		  oldangle2,
  		  posArmX, posArmY, refPosX, refPosY,
  		  pi = 3.14159265359,
  		  arm1Length,		//length of inner robot arm
  		  arm2Length, 	//length of outer robot arm
          ldummy,
  		  zeroAngle = 0,
          a1,a2,
  		  diff1, diff2,
  		  x ,xr,
  		  y ,yr;
  		
    bool  claw, clawOpen,cl;
    int	i, 
  		del, 
  		steps;
  		
    int iAction=1; 
    
    dpGet(\"ScaraRobot.xpos:_online.._value\",x,
          \"ScaraRobot.ypos:_online.._value\",y,
          \"ScaraRobot.steps:_online.._value\",steps,
          \"ScaraRobot.stepdelay:_online.._value\",del,
          \"ScaraRobot.clawopen:_online.._value\",clawOpen);
          
       
    getMultiValue(\"armLength1\",\"size\",arm1Length,ldummy,
                  \"armLength2\",\"size\",arm2Length,ldummy,
  				  \"scaraLink0\",\"position\",refPosX, refPosY,
  				  \"scaraBase\",\"position\",xr,yr);

    setValue(\"posInd\",\"position\",xr+x,yr+y);  				

    currentPosition(angle1, angle2, claw);
  
    oldangle1 = angle1;
    oldangle2 = angle2;

    do
    {
      calcAngles(x, y, arm1Length, arm2Length, angle1, angle2, angle1a, angle2a); 
    
      //optimize the robot-trajectory
      if(iAction==1)
      {
        float rinit, ainit; //initial position
        float rtarget,atarget; //target position
        float xinit,yinit;
        int iSolution, //the optimal soltuion for the robot-trajectory
            iLastSolution; //the last optimal soltuion for the robot-trajectory
        string s;
 
        iAction++;
      
        //target-position
        r2p(x,y,rtarget,atarget);
      
        //initial-position
        xinit = arm1Length*cos(deg2rad(oldangle1)) + arm2Length*cos(deg2rad(oldangle2));
        yinit = arm1Length*sin(deg2rad(oldangle1)) + arm2Length*sin(deg2rad(oldangle2));
        yinit=-yinit;
        r2p(xinit,yinit,rinit,ainit);
 
        sprintf(s,\"%5.2f\",ainit);
        ainit=s;
        sprintf(s,\"%5.2f\",atarget);
        atarget=s;
        sprintf(s,\"%5.2f\",rinit);
        rinit=s;
        sprintf(s,\"%5.2f\",rtarget);
        rtarget=s;
        
        //if the same Position then not move
        if(ainit==atarget && rinit == rtarget && clawOpen==claw)
        {
          dpSet(\"ScaraRobot.move:_original.._value\",0);
          break;
        }
        else if(ainit==atarget && rinit == rtarget && clawOpen!=claw)
        {
          moveRobot(oldangle1, oldangle2, clawOpen);
          dpSet(\"ScaraRobot.move:_original.._value\",0);
          break;
        }
      
        if(ainit>-180 && ainit<0 ) ainit=180+ainit;
        else if(ainit<-180 ) ainit=360+ainit;
      
        getValue(\"lastSolution\",\"text\",iLastSolution);      
        if(iLastSolution!=0 && ((-90 < alpha && alpha < 90) || 270 < alpha  || alpha < -270))     // angle between arm1 (r1) and arm2 (r2))
          iSolution=iLastSolution;             
        else if((ainit-180 < atarget && atarget < ainit) || (ainit+180 < atarget && atarget < 360))  
        {
          //the second solution is better
          iSolution=2;
        }
        else
        {
          //the first solution is better
          iSolution=1;
        }
      
        if(iSolution==1)
        {
          angleOpt1=angle1;
          angleOpt2=angle2;
        }
        else
        {
          angleOpt1=angle1a;
          angleOpt2=angle2a;      
        }
        setValue(\"lastSolution\",\"text\",iSolution);
      }
      if ( (oldangle1 != angleOpt1) ||  (oldangle2 != angleOpt2) || (clawOpen!=claw)) 
      {
          diff1 = angleOpt1 - oldangle1;
          diff2 = angleOpt2 - oldangle2;
          if(diff1 > 180)
            diff1 = diff1 - 360;
          else if(diff1 < - 180)
            diff1 = diff1 + 360;
      
          if(diff2 > 180)
            diff2 = diff2 - 360;
          else if(diff2 < - 180)
            diff2 = diff2 + 360;
          for( i=1 ; i <= steps ; i++)
          {
            angleOpt1 = oldangle1 + i*(diff1)/steps;
            angleOpt2 = oldangle2 + i*(diff2)/steps;
            moveRobot(angleOpt1, angleOpt2, clawOpen);
            currentPosition(a1,a2,cl);
            delay(0,del);
          }
      }    
      oldangle1 = angleOpt2;
      oldangle2 = angleOpt2;
      dpSet(\"ScaraRobot.move:_original.._value\",0);
      break;
    }while(true);
  }//end else
}//end work-function

moveRobot(float angle1, float angle2, bool clawOpen)
{
  // refPosX/Y, arm1/2Length are read in main
  float posArmX, posArmY, // refPosX, refPosY,
  		// arm1Length,  		//length of inner robot arm
  		// arm2Length, 	    //length of outer robot arm
  		zeroAngle = 0,
  		maxAngleDiff = 115,
  		AngleDiff,
		  x,y;  
  
 				
  // Convert all angles into 0-360°  
  angle1 = baseAngle(angle1);
  angle2 = baseAngle(angle2);
  
  posArmX = refPosX + arm1Length*cos(deg2rad(angle1 + zeroAngle));
  posArmY = refPosY - arm1Length*sin(deg2rad(angle1 + zeroAngle));

  AngleDiff = (fabs(angle2-angle1) <= 180) ? fabs(angle2-angle1) : 360 - fabs(angle2-angle1);    
  if ( AngleDiff <= maxAngleDiff)     
    setMultiValue(\"scaraArm1\",\"rotation\",angle1,
  				  \"Arm.scaraArm2\",\"rotation\",angle2,
  				  \"Arm.scaraClamb1\",\"rotation\",clawOpen?angle2:angle2+2,
  				  \"Arm.scaraClamb2\",\"rotation\",clawOpen?angle2:angle2-2,
  				  \"Arm\",\"position\",posArmX, posArmY,
  				  //\"scaraArm1\",\"backCol\",\"grey\",
  				  //\"object_0\",\"visible\",clawOpen?0:1,   //the last two lign are only for the Demo
  				  \"object_0\",\"position\",posArmX+arm2Length*cos(deg2rad(angle2)),
  				                        posArmY-arm2Length*sin(deg2rad(angle2))); 
  else
    setMultiValue(\"scaraArm1\",\"rotation\",angle1,
  				  \"Arm.scaraArm2\",\"rotation\",angle2,
  				  \"Arm.scaraClamb1\",\"rotation\",clawOpen?angle2:angle2+2,
  				  \"Arm.scaraClamb2\",\"rotation\",clawOpen?angle2:angle2-2,
  				  \"Arm\",\"position\",posArmX, posArmY,
  				  \"scaraArm1\",\"backCol\",\"red\",
  				  //\"object_0\",\"visible\",clawOpen?0:1,   //the last two lign are only for the Demo
  				  \"object_0\",\"position\",posArmX+arm2Length*cos(deg2rad(angle2)),
  				                        posArmY-arm2Length*sin(deg2rad(angle2))); //the last lign is only for the Demo
			  
    				  
  				  
  // getMultiValue(\"scaraArm1\",\"rotation\",angle1,
  //        	   	 \"Arm.scaraArm2\",\"rotation\",angle2);

  x = arm1Length*cos(deg2rad(angle1)) + arm2Length*cos(deg2rad(angle2));
  y = arm1Length*sin(deg2rad(angle1)) + arm2Length*sin(deg2rad(angle2));
}//end moveRobot()

calcAngles(float x, float y, float r1, float r2, float &angle1, float &angle2, float &angle1a, float &angle2a)
{
  float r0, a0,
        gamma,		// angle between arm1 (r1) and target arm (r0)
    	pi = 3.14159265359;
  
  r2p(x,y,r0,a0);
  
  y = -y; //conversion from panelcoordinates to mathematical coordinates
  
  // Take care of rounding errors
  if (r0 > r1 + r2)
    r0 = r1 + r2;  
  if (r2 > r0 + r1)
    r2 = r0 + r1;
  
  alpha = rad2deg (acos(( (pow(r1,2) + pow(r2,2) - pow(r0,2)) / (2*r1*r2)) ) );
  
  if (r0 != 0)
    gamma = rad2deg (acos(( (pow(r1,2) + pow(r0,2) - pow(r2,2)) / (2*r1*r0)) ) );
  else
    gamma = 0;
  angle1 = a0 - gamma;
  angle1a = a0 + gamma;
  
  angle2 = 180 - (alpha-angle1);
  angle2a =-180+ (alpha + angle1a);
}//end calcAngles()

currentPosition(float &angle1, float &angle2, bool &claw)
{
  float clawRot,angle;
  string s;
  getMultiValue(\"scaraArm1\",\"rotation\",angle1,
  				\"Arm.scaraArm2\",\"rotation\",angle2,
  				\"Arm.scaraClamb1\",\"rotation\",clawRot);

  sprintf(s,\"%5.2f\",angle2);
  angle=s;
  sprintf(s,\"%5.2f\",clawRot);
  clawRot=s;
  if(angle == clawRot)
    claw = 1;
  else
    claw=0;
}//end currentPosition


// converts any angle into the interval [0..360]
float baseAngle(float angle)
{
  if (angle>0)
    angle = fmod(angle,360);
  else
    angle = 360+fmod(angle,360);
  return(angle);
}//end baseAngle

// converts cartesian (x,y) coordinates into
// polarcoordinates (r,a); caution! positiv 
// y axis points downwards like in panel coordinates
r2p(float x, float y, float &r, float &a)
{
  r = sqrt(pow(x,2)+pow(y,2));
  
  if (x > 0)
    a = -rad2deg(atan(y/x));
  else if (x < 0)
    a = 180 - rad2deg(atan(y/x));
  else if (x == 0)
    a = (y <= 0) ? 90 : 270;
}//end r2p()
   
// converts polar coordinates (r,a) into cartesian
// (x,y) coordinates; caution! positiv y axis 
// points downwards like in panel coordinates
p2r(float &x, float &y, float r, float a)
{	
  x = r*cos(deg2rad(-a));
  y = r*sin(deg2rad(-a));
