"main()
{
  float     kR, TN, TV, u_min, u_max, antiwindup, tolerance, scale;
  int	    mode, scanrate, i;
  string    w, u, x, w_, u_, x_, sys, thisType = substr(dpSubStr($controller,DPSUB_DP), 4, strlen(dpSubStr($controller,DPSUB_DP))-4),
            thisSys = substr(dpSubStr($controller,DPSUB_DP), 0, 4);
  dyn_float fanswer;
  dyn_string reactors = dpNames(\"*LICA*\",\"PID_Control\"), sanswer;

  for(i=1; i<=dynlen(reactors); i++)
  {
    sprintf(sys,\"R%02d_\",i);
    reactors[i] = sys;
  }
  
  if(dynlen(reactors) > 1)
  {
    ChildPanelOnCentralModalReturn(\"vision/messageWarning2\",getCatStr(\"general\",\"warning\"), 
         makeDynString(\"$1:\"+getCatStr(\"control\",\"really_distribute_settings_1\")+dynlen(reactors)+
         +getCatStr(\"control\",\"really_distribute_settings_2\")+ thisType + getCatStr(\"control\",\"really_distribute_settings_3\"),
         \"$2:\"+getCatStr(\"general\",\"OK\"),\"$3:\"+getCatStr(\"general\",\"cancel\")),
         fanswer, sanswer);
  }
  else
    return;
    

  if(fanswer[1] != 0)
  {
    dpGet( 	$controller+\".parameters.kR\",kR,
  			$controller+\".parameters.TN\",TN,
  			$controller+\".parameters.TV\",TV,
  			$controller+\".parameters.u_min\",u_min,
  			$controller+\".parameters.u_max\",u_max,
  			$controller+\".parameters.antiwindup\",antiwindup,
  			$controller+\".parameters.tolerance\",tolerance,
  			$controller+\".parameters.scale\",scale,
  			$controller+\".parameters.mode\",mode,
  			$controller+\".parameters.scanrate\",scanrate,
  			$controller+\".input.w\",w,	
  			$controller+\".input.x\",x,
  			$controller+\".output.u\",u);	
  				
    for(i=1; i<=dynlen(reactors); i++)
    {
      w_ = w;
      x_ = x;
      u_ = u;
  
      strreplace(w_,thisSys,reactors[i]);
      strreplace(x_,thisSys,reactors[i]);
      strreplace(u_,thisSys,reactors[i]);
     
      dpSet(reactors[i]+thisType+\".parameters.kR\",kR,
  			reactors[i]+thisType+\".parameters.TN\",TN,
  			reactors[i]+thisType+\".parameters.TV\",TV,
  			reactors[i]+thisType+\".parameters.u_min\",u_min,
  			reactors[i]+thisType+\".parameters.u_max\",u_max,
  			reactors[i]+thisType+\".parameters.antiwindup\",antiwindup,
  			reactors[i]+thisType+\".parameters.tolerance\",tolerance,
  			reactors[i]+thisType+\".parameters.scale\",scale,
  			reactors[i]+thisType+\".parameters.mode\",mode,
  			reactors[i]+thisType+\".parameters.scanrate\",scanrate,
  			reactors[i]+thisType+\".input.w\",w_,	
  			reactors[i]+thisType+\".input.x\",x_,
  			reactors[i]+thisType+\".output.u\",u_);	
    }
  }  
}" 0
