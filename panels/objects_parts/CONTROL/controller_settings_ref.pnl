"//Function cTrend_EventInitialize()
main()
{  
  string w_dpe;
  float  min, max;

  dpGet($controller+\".input.w\",w_dpe);
  dpGet(w_dpe+\":_pv_range.._min\",min,
        w_dpe+\":_pv_range.._max\",max);


  switch(g_devZoomMode)
  {
    case(1): {	max = max - (max-min)*0.5;
                break;
             }
    case(2): {	max = max - (max-min)*0.8;
                break;
             }
    case(3): {	max = max - (max-min)*0.95;
                break;
             }  
    default:  break;
  }
  g_devZoomMode++;
  
  if (g_devZoomMode > 3)
     g_devZoomMode = 0;   

  cTrend.curveMax(\"#1_4\") = max-min;
  cTrend.curveMin(\"#1_4\") = 0-(max-min);
}" 0
