"// This script allows \"fit to module\" zooming
main()
{
  dyn_int pnlSize;
  float   factor, xFactor, yFactor, 
          mwidth = 854, mhight = 560;   // width and height of the module in pixels
  float   maxFactor, minFactor;
  
  getZoomRange(minFactor, maxFactor);
  
  // read the pixel size of the panel
  pnlSize = getPanelSize(CurrentPanelFileBox.text);
  
  // determine the factors for \"fit to module\"
  xFactor = mwidth / pnlSize[1];
  yFactor = mhight / pnlSize[2];
  
  
  // if proportions of new panel and module do not fit,
  // fit so that everything is visible...
  factor = (xFactor < yFactor) ? xFactor : yFactor;
  if (factor > maxFactor)
    factor = maxFactor;
  else
    if (factor < minFactor)
      factor = minFactor;

  // Zoom the module by \"factor\" but do not pan: \"0,0\"
  ZoomModule(\"ZoomWindow\",factor,0,0);

}" 0
