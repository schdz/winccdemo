  float  zoomStep, currentFactor, minFactor, maxFactor;
  string szoomStep = StepSelector.text;
  
  // cutoff unit and blanks, step is now e.g. 15
  zoomStep = strrtrim(szoomStep,\" %\");
  
  // normalize step from +15% to 1.15 as factor
  zoomStep = 1 + zoomStep / 100;
  
  getZoomFactor(currentFactor,\"ZoomWindow\");
  getZoomRange(minFactor, maxFactor);
  
 
  if (currentFactor * zoomStep >= maxFactor)
    zoomStep = (maxFactor-0.001) / currentFactor;
    

  panelZoomIn(\"ZoomWindow\",zoomStep);

