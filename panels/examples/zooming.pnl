"main()
{
  string     ModuleName = \"ZoomWindow\";

  dpConnect(\"zoomWorkCB\",myUiDpName()+\".Zoom.ModuleName:_online.._value\",
                           myUiDpName()+\".Zoom.Factor:_online.._value\",
                           myUiDpName()+\".Zoom.Set:_online.._value\");
                         
  ZoomCombo.items = makeDynString(\"  50 %\", \"  75 %\", \"100 %\", \"115 %\", \"125 %\", \"150 %\", \"200 %\", \"400 %\");
                        
}
zoomWorkCB(string dp1, string moduleName, string dp2, float factor, string dp3, int iSet)
{
  int    iFactor;
  string sFactor;
  
  if (moduleName != \"ZoomWindow\") return;

  // the module on the right side labled \"ZoomWindow\" is zoomed
  factor *= 100; iFactor = factor + 0.5;
  sprintf(sFactor,\"%d\",iFactor);
  sFactor += \" %\";
  this.text = sFactor;
}" 0

"main()
{ 
  string    sFactor = this.text;
  float		fFactor, maxFactor, minFactor;
  int		iFactor;
  
  
  
  // calculate numeric factor e.g. 1.15 out of user
  // input e.g. 25%
  strreplace(sFactor,\" %\",\"\");
  fFactor = sFactor;
  iFactor = fFactor + 0.5; if (iFactor <= 0) iFactor = 100;
  sprintf(sFactor,\"%d\",iFactor); sFactor += \" %\";
  this.text = sFactor;
  fFactor = iFactor/100.0;
  
  getZoomRange(minFactor, maxFactor);
  
    if (fFactor > maxFactor)
    fFactor = maxFactor;
  else
    if (fFactor < minFactor)
      fFactor = minFactor;
  
  
  // zoom module with name \"ZoomWindow\" by the factor fFactor
  // and move it 0 points left and 0 points down (no panning)
  ZoomModule(\"ZoomWindow\",fFactor,0,0);  
  
  
}" 0

