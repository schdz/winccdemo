"main()
{
  bool 	      loopOn;
  int		  i = 1, loopTime;
  string      filepath;
  dyn_string  fileList;

  // wait until input section elements are ready initialized
  delay(0,500);
  
  // get on-state form backCol of the \"LED\" shape... line **
  if(LoopLED.backCol == \"STD_led_On\")
    loopOn = true;
  else
    loopOn = false;
    
  filepath = Input_Path.text;
  
  while(true)  // endless loop - poll on-state for slideshow
  {  
    if(loopOn)
    {
      if(filepath != \"\")
      {
        fileList = Input_File.items;
        if (dynlen(fileList) > 1)
        {
          enablePictureSelection(false);
          enableAllShapes(false);
          filepath = Input_Path.text;
          
          while(true)	// inner loop - if slideshow is on, loop all pictures in directory
          {			    // which apply to the current file type filter
            loopTime = strrtrim(LoopTime.text,\" s\");
       
            if(i > dynlen(fileList))  // loop endless - begin again when reached the end
              i = 1; 

            if (LoopLED.backCol == \"STD_led_Off\")
            {
              break;  // leave immediately inner loop if slideshow is switched off
            }
            Input_File.text = fileList[i];  // show current filename in combobox
            loadPicture(fileList[i],filepath);  // display the picture file
            delay(loopTime);
            
            i++;
          }
        }
        else
          enablePictureSelection(true);
          enableAllShapes(true);
      }
    }
    else
    {
      loopOn = (LoopLED.backCol == \"STD_led_On\")?true:false;  // short version of line **
    } 
    loopOn = (LoopLED.backCol == \"STD_led_On\")?true:false;
    delay(1);
  }  
