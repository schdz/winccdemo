"main()
{
  string       path = Input_Path.text, pattern, lastFile, lastFileType;
  dyn_string   fileListFull;
  
  if(path != \"\")
  {
    if(this.selectedPos == 1)
    {
      dyn_string ds2, ds = getFileNames(path,\"*.*\");
      
      for (int i = 1; i <= dynlen(ds); i++)
      {
        dyn_string ds3 = strsplit(ds[i], \".\");
        if (ds3[dynlen(ds3)] != \"xbm\")
          dynAppend(ds2, ds[i]);
      }
      Input_File.items = ds2;
      
      if (dynlen(ds2) <= 0)
        Input_File.text = \"NONE\";
      else
        Input_File.text = ds2[1];
    }
    else
    {
      // if a specific file type has been chosen
      pattern = \"*.\"+strtolower(this.text);
      fileListFull = getFileNames(path,pattern);
      
      if (dynlen(fileListFull) <= 0)
      { 
        dyn_string ds;
        Input_File.items = ds;
        Input_File.text = \"NONE\";
      }
      else
      {
        Input_File.items = fileListFull;
      
        lastFile = Input_File.text;
        if(lastFile != \"\")
        {
          // if new file type selection matches to the
          // currently diplayed picutre - don´t change
          lastFileType = getFileExtension(lastFile);
        
          // if filetypes do not match, display first 
          // picture in the new selection list...
          if (lastFileType != strtolower(this.text))
          {
            Input_File.text = fileListFull[1];
            loadPicture(fileListFull[1],Input_Path.text);
          } 
        }
      }
    }
    
    // WMF and EMF files can not be displayed in pushbuttons !
    if(this.text == \"WMF\" || this.text == \"EMF\")
      setMultiValue(\"InclPushbutton\",\"enabled\",false,
                    \"InclPushbutton\",\"state\",0,false,
                    \"PicturePushbutton\",\"text\",\"NOT SUPPORTED\");
    else
      setMultiValue(\"InclPushbutton\",\"enabled\",true);            
  }
}" 0

