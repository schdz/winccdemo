int xmin = 30, xmax = 410, ymin = 60, ymax = 420;
int xstep = 30, ystep = 30, origwidth = 189, origheight = 179;

// definition of functions
// ===============================================
loadPicture(string filename, string path, bool showdebugs = false)
{
  bool fit = true;
  int  size;
  string filetype;

  filetype = getFileExtension(filename);
  
  if (filetype == \"emf\")
    filetype = \"wmf\";
    
  if (filetype == \"xbm\")
    return;
  
  fit = fit_selector.number;
  fit = !fit;
  

  if (access(filename + path,R_OK))
  {
    size = getFileSize(path + filename);
    FileSize.text = byteSizeToString(size);
    if(InclRectangle.state(0))
    {
      PictureRectangle.fill = \"[solid]\";
      if(showdebugs)      
        DebugN(\"CMD=> PictureRectangle.fill = \"+\"[pattern,[\"+ ((fit)?\"fit\":\"tile\")+\",\"+filetype+\",\" + path + filename +\"]]\");
      PictureRectangle.fill = \"[pattern,[\"+ ((fit)?\"fit\":\"tile\")+\",\"+filetype+\",\" + path + filename +\"]]\";
    }
    
    if(InclPushbutton.state(0))
    {
      //PicturePushbutton.fill = \"[solid]\";  // never set pushbuttons to \"solid\" !!!
      if (filetype == \"wmf\" || filetype == \"emf\")
        PicturePushbutton.text = \"NOT SUPPORTED\";
      else
      {
        if(showdebugs)      
          DebugN(\"CMD=> PicturePushbutton.fill = \"+\"[pattern,[\"+ ((fit)?\"fit\":\"tile\")+\",\"+filetype+\",\" + path + filename +\"]]\");
        PicturePushbutton.fill = \"[pattern,[\"+ ((fit)?\"fit\":\"tile\")+\",\"+filetype+\",\" + path + filename +\"]]\";
      }
    }
    
    if(InclPolyline.state(0))
    {
      PicturePolyLine.fill = \"[solid]\";
      if(showdebugs)      
        DebugN(\"CMD=> PicturePolyLine.fill = \"+\"[pattern,[\"+ ((fit)?\"fit\":\"tile\")+\",\"+filetype+\",\" + path + filename +\"]]\");
      PicturePolyLine.fill = \"[pattern,[\"+ ((fit)?\"fit\":\"tile\")+\",\"+filetype+\",\" + path + filename +\"]]\";
    }

    if(InclCircle.state(0))
    {
      PictureCircle.fill = \"[solid]\";
      if(showdebugs)      
        DebugN(\"CMD=> PictureCircle.fill = \"+\"[pattern,[\"+ ((fit)?\"fit\":\"tile\")+\",\"+filetype+\",\" + path + filename +\"]]\");
      PictureCircle.fill = \"[pattern,[\"+ ((fit)?\"fit\":\"tile\")+\",\"+filetype+\",\" + path + filename +\"]]\";
    }

  }
}

string getFileExtension(string fileOrPath)
{
  dyn_string splitString;
  
  if(fileOrPath != \"\")
  {
   splitString = strsplit(fileOrPath,\".\");
   return strtolower(splitString[dynlen(splitString)]);
  }
  else
    return \"-1\";
}

enableAllShapes(bool all)
{
  if(all)
    setMultiValue(	\"InclCircle\",\"enabled\",true,
    				\"InclPolyline\",\"enabled\",true,
    				\"InclPushbutton\",\"enabled\",true,
    				\"InclRectangle\",\"enabled\",true);
  else
    setMultiValue(	\"InclCircle\",\"enabled\",false,
			        \"InclCircle\",\"state\",0,false,
    				\"InclPolyline\",\"enabled\",false,
    				\"InclPolyline\",\"state\",0,false,
    				\"InclPushbutton\",\"enabled\",false,
    				\"InclPushbutton\",\"state\",0,false,
    				\"InclRectangle\",\"enabled\",true);
}

enablePictureSelection(bool enable)
{
  if(enable)
    setMultiValue( \"Input_File\",\"enabled\",true,
                   \"Input_Path\",\"enabled\",true,
                   \"Input_Type\",\"enabled\",true,
                   \"OpenFileSelector\",\"enabled\",true);
  else
    setMultiValue( \"Input_File\",\"enabled\",false,
                   \"Input_Path\",\"enabled\",false,
                   \"Input_Type\",\"enabled\",false,
                   \"OpenFileSelector\",\"enabled\",false);      
}

// scale functions for the rectangular picture shape
// default parameters = \"-1\" means: \"read values from 
// input-boxes !
scaleShape(float xpercent, float ypercent = -1)
{
  bool prop;
  string xstr, ystr;   
  
  getMultiValue(\"ScaleProp\",\"state\",0,prop,
                \"xscale\",\"text\",xstr,
                \"yscale\",\"text\",ystr);

  if(xpercent == -1)
  {
    xstr = strrtrim(xstr,\" %\");
    xpercent = xstr;
  }
  if(ypercent == -1)
  {
    ystr = strrtrim(ystr,\" %\");
    ypercent = ystr;
  }

  xpercent = xpercent/100.0;
  ypercent = ypercent/100.0;
     
  if(prop)
    setValue(\"PictureRectangle\",\"scale\", xpercent, xpercent); 
  else
    setValue(\"PictureRectangle\",\"scale\", xpercent, ypercent); 
    
}

// moves the specified shape inside the through xmin,xmax,ymin,ymax
// defined bounding box...
int moveShape(int xmove, int ymove, string shapename = \"PictureRectangle\", int steps = 3)
{
   float x,y, width, height, xnew, ynew;
   bool  xcollision, ycollision;
   int   i;
   
   getValue(shapename,\"position\",x,y,
                      \"size\",width,height);
   
   if (xmove < 0)
   {   
     if ((x + xmove) < xmin)
     {
       xnew = xmin;
       xcollision = true;       
     }
     else
       xnew = x + xmove;
   }
   else
   {
     if ((x + xmove + width) > xmax)
     {
       xnew = xmax - width;
       xcollision = true;       
     }
     else
       xnew = x + xmove;   
   }  

   if (ymove > 0)
   {   
     if ((y + ymove) > ymax)
     {
       ynew = ymax;
       ycollision = true;       
     }
     else
       ynew = y + ymove;
   }
   else
   {
     if ((y + ymove - height) < ymin)
     {
       ynew = ymin + height;
       ycollision = true;       
     }
     else
       ynew = y + ymove;   
   }  
   
   if (steps <= 1)
     setValue(shapename,\"position\",xnew,ynew);
   else
   {
     for(i = steps+1; i > 1; i--)
     {
       setValue(shapename,\"position\",x + ((xnew-x)/i),y + ((ynew-y)/i));
       delay(0,40);
     }    
   }  
   return xcollision;
}

// limit resize-operation when not in original position
limitScaling(int &xs, int &ys, string shapename = \"PictureRectangle\")
{
   float x,y, xscale = xs/100.0, yscale = ys/100.0;
   
   getValue(shapename,\"position\",x,y);
   
   if ((x + origwidth*xscale) > xmax)                   
     xscale = 100.0 * ((xmax - x)/origwidth);
   else
     xscale = xscale * 100.0;
   if ((y - origheight*yscale) < ymin)
     yscale = 100.0 * ((y - ymin)/origheight);
   else
     yscale = yscale * 100.0;

   xs = xscale;
   ys = yscale;
}

