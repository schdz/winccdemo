//********************************
OpenFileSelector()
{
  string     filestr, 
             dirstr, 
             pattern, 
             ext,
             filename, 
             dirname;
  dyn_string pathparts;
  
  // determine which filetype (bmp, xpm, wmf,...) is selected
  if(Input_Type.selectedPos == 1)
    pattern = \"*.*\";
  else
    pattern = \"*.\"+strtolower(Input_Type.text); // create search pattern for directory scan
  
  // if nothing else is specified, set current directory to currents project /pictures/ path
  dirstr = (Input_Path.text == \"\") ? PROJ_PATH+\"pictures\" : Input_Path.text;
 
  strreplace(dirstr,\"\\\\\",\"/\");
  
  // open a fileselector 
  fileSelector (filestr, dirstr, false, pattern);
  
  // if fileselector is canceled do nothing (leave old settings)
  if(filestr == \"\"  || filestr == \"\")
    return;


  // split full file and pathstring in all sections separated by delimiter \"/\" 
  pathparts = strsplit(filestr, \"/\");
  
  // filename is the last section
  filename = pathparts[dynlen(pathparts)];
  // directory is the whole filestr if file itself is cut off at the end
  dirname = strrtrim(filestr, pathparts[dynlen(pathparts)]);

  ext = strtoupper( substr(filename, strlen(filename)-3, 3));
  
  if ( ext == \"WMF\" ||  ext == \"EMF\" || ext == \"XPM\" ||  ext == \"BMP\" ||  ext == \"GIF\" )
  {
    // write file and path selections to the text-/inputfields
    Input_Path.text = dirname;
    Input_File.items = getFileNames(dirname,pattern);
    Input_File.text = filename;
  
    // load selceted graphics file / picture
    // load operation is a \"panel local library function\" - this
    // can be found in the \"General\" section of the \"Events\" tab in the 
    // panels property sheet (Edit -> Properties)
    loadPicture(filename, dirname);
  }
  else
  {
    ChildPanelOnCentralModal(\"vision/MessageWarning\",getCatStr(\"DemoApp\",\"warning\"),makeDynString(\"$1:\"+getCatStr(\"DemoApp\",\"path_incorrect\")));
  }
}
