"main()
{
  float xcenter,ycenter, xmove, ymove, x, y, width, height;
  
  xcenter = xmin + (xmax-xmin)/2.0;
  ycenter = ymin + (ymax-ymin)/2.0;
  
  getValue(\"PictureRectangle\",\"position\",x,y,
                              \"size\",width,height);
 
  if (xcenter > (x + width/2.0))
    xmove = xcenter - (x + width/2.0);
  else
    xmove = xcenter - (x + width/2.0);
    
  if (ycenter > (y - height/2.0))
    ymove = ycenter - (y - height/2.0); 
  else
    ymove = ycenter - (y - height/2.0); 
   
  moveShape(xmove, ymove, \"PictureRectangle\", 0);
}" 0
