"bool On;

main()
{
  if(!dpExists(\"ExampleDP_OnOff.:_online.._value\"))
    setValue(\"\",\"backCol\",\"STD_dp_not_existing\",
                \"foreCol\",\"STD_dp_not_existing\");
  else
    dpConnect(\"work\",\"ExampleDP_OnOff.:_online.._value\");

  if(!dpExists(\"ExampleDP_Knob.:_online.._value\"))
    setValue(\"horiSlide\",\"backCol\",\"STD_dp_not_existing\");
  else
    dpConnect(\"Knob\",\"ExampleDP_Knob.:_online.._value\");

  DoSimulation();
}

work(string dp1, int value)
{
  On = (value != 0) ? TRUE : FALSE;
}

Knob(string dp1, int value)
{
  dpSet(\"ExampleDP_Scale.:_original.._value\",value);
}

DoSimulation()
{
  int  j,k;
  bool wasActive = FALSE;
  
  j = k = 0;
  while(TRUE)
  {
    if(On)
    {
      wasActive = TRUE;
      (j <100) ? j++ : (j = 0);
      (k <360) ? (k += 5) : (k = 0);     
      dpSet(\"ExampleDP_Scale.:_original.._value\",  j,
            \"ExampleDP_Move.:_original.._value\",  k);
      delay(0,70);
    }
    else
    {
      if(wasActive)
      {
        j = k = 0;
        dpSet(\"ExampleDP_Scale.:_original.._value\",  j,
              \"ExampleDP_Move.:_original.._value\",  k,
              \"ExampleDP_Knob.:_original.._value\",  0);
        wasActive = FALSE;
      }
      delay(1,500);
    }
  }
}
