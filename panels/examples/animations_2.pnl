work(string dp, int value)
{
  string fgColor     = \"[100,0,0]\",
         bgColor     = \"[60,100,100]\",
         hatch       = \"[solid]\",
         linestyle   = \"[solid,oneColor,JoinMiter,CapButt,4]\";
  bool   isVisible   = TRUE;
         
  if(value == 1) //change forecol static
  {
    getValue(\"foreGnd\",\"backCol\",fgColor);
  }
  else if(value == 2) //change backcol static
  {
    getValue(\"backGnd\",\"backCol\",bgColor);
  }
  else if(value == 3) //change forecol animated
  {
    getValue(\"foreGndbl\",\"backCol\",fgColor);
  }
  else if(value == 4) //change backcol animated
  {
    getValue(\"backGndbl\",\"backCol\",bgColor);
  }
  else if(value == 5) //change hatch style
  {
    getValue(\"hatched\",\"fill\",hatch);
    bgColor = \"[0,0,0]\";
  }
  else if(value == 6) //transparent
  {
    hatch = \"[outline]\";
  }
  else if(value == 7) //set linestyle
  {
    getValue(\"lineStyle\",\"border\",linestyle);
  }
  else if(value == 8) //visibility
  {
    isVisible = FALSE;
  }

  //set attributes
  setValue(\"\",\"foreCol\",fgColor,
              \"backCol\",bgColor,
              \"visible\",isVisible,
              \"border\" ,linestyle,
              \"fill\"   ,hatch);
 
}" 0
